<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="oval">
            <solid android:color="#28292c" />
            <size android:width="26dp" android:height="26dp" />
        </shape>
    </item>
    <item>
        <layer-list>
            <item>
                <shape android:shape="oval">
                    <solid android:color="#28292c" />
                    <size android:width="26dp" android:height="26dp" />
                </shape>
            </item>
            <item
                android:drawable="@drawable/ic_thumb_moon"
                android:top="4dp"
                android:bottom="4dp"
                android:left="4dp"
                android:right="4dp"/>
        </layer-list>
    </item>
</selector>
